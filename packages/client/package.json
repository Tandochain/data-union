{
    "name": "@dataunions/client",
    "version": "1.0.0",
    "description": "JavaScript client library for DataUnions",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dataunions/data-union.git",
        "directory": "packages/client"
    },
    "types": "./dist/types/src/index.d.ts",
    "main": "./dist/src/index-commonjs.js",
    "exports": {
        "default": {
            "import": "./dist/src/index-esm.mjs",
            "require": "./dist/src/index-commonjs.js"
        }
    },
    "scripts": {
        "build": "npm run bootstrap-dist && npm run build-node",
        "build-production": "npm run clean; npm run bootstrap-dist && NODE_ENV=production npm run build-node",
        "check": "tsc -p ./tsconfig.json --noEmit",
        "vendor": "SRCDIR=`node -p \"path.dirname(require.resolve('quick-lru'))\"` bash -c 'mkdir -p vendor/quick-lru; cp -n $SRCDIR/index.d.ts vendor/quick-lru; cp -n $SRCDIR/index.js vendor/quick-lru'; true",
        "copy-package": "mkdir -p dist/; node copy-package.js; cp -f README.md LICENSE readme-header-img.png dist; true",
        "bootstrap-dist": "npm run vendor && npm run copy-package && npm run fix-esm",
        "fix-esm": "mkdir -p dist/src/; cp -Rf src/index-esm.mjs dist/src/index-esm.mjs",
        "build-node": "npm run bootstrap-dist && tsc --build ./tsconfig.node.json",
        "postbuild-node": "npm run copy-package",
        "lint": "eslint --cache --cache-location=node_modules/.cache/.eslintcache/ '*/**/*.{js,ts}'",
        "test-integration": "jest --maxWorkers=1 test/end-to-end",
        "clean": "jest --clearCache || true; rm -rf dist vendor *.tsbuildinfo node_modules/.cache || true"
    },
    "engines": {
        "node": ">= 16"
    },
    "author": "Streamr",
    "license": "Apache-2.0",
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/plugin-proposal-decorators": "^7.16.4",
        "@babel/plugin-transform-modules-commonjs": "^7.16.0",
        "@babel/plugin-transform-runtime": "^7.16.4",
        "@babel/preset-env": "^7.16.4",
        "@babel/preset-typescript": "^7.16.0",
        "@dataunions/dev-config": "^1.0.0",
        "@dataunions/eslint-config": "^1.0.0",
        "@ethersproject/wordlists": "^5.5.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.2",
        "@types/lodash": "^4.14.175",
        "@types/node": "^16.10.6",
        "@types/node-fetch": "^2.5.12",
        "@types/split2": "^3.2.1",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "babel-loader": "^8.2.3",
        "babel-plugin-add-module-exports": "^1.0.4",
        "babel-plugin-lodash": "^3.3.4",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "benchmark": "^2.1.4",
        "buffer": "^6.0.3",
        "bytes": "^3.1.1",
        "chromedriver": "^100.0.0",
        "concurrently": "^6.4.0",
        "cors": "^2.8.5",
        "crypto-browserify": "^3.12.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-promise": "^5.1.0",
        "ethers": "^5.4.7",
        "express": "^4.17.1",
        "git-revision-webpack-plugin": "^5.0.0",
        "jest": "^27.2.5",
        "jest-circus": "^27.2.5",
        "jest-extended": "^0.11.5",
        "jest-leak-detector": "^27.3.1",
        "lodash-webpack-plugin": "^0.11.6",
        "nightwatch": "^1.7.12",
        "node-polyfill-webpack-plugin": "^1.1.4",
        "pretty-bytes": "^5.6.0",
        "process": "^0.11.10",
        "streamr-test-utils": "^2.0.0",
        "terser-webpack-plugin": "^5.2.5",
        "ts-jest": "^27.0.5",
        "ts-loader": "^9.2.6",
        "typedoc": "^0.22.5",
        "typescript": "^4.5.2",
        "util": "^0.12.4",
        "weak-napi": "^2.0.2",
        "webpack": "^5.64.1",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.1",
        "webpack-merge": "^5.8.0"
    },
